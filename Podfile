source 'https://github.com/CocoaPods/Specs.git'

platform :ios,'11.3'
use_frameworks!
inhibit_all_warnings!

def pods
  
  # Network
  pod 'Alamofire', '~> 4.8'
  
  # Map
  pod 'MapboxNavigation', '~> 0.32'
  
  # Layout / UI
  pod 'LGButton', '~> 1.1'
  pod 'SwiftMessages', '~> 6.0'
  
  # Utility
  pod 'CryptoSwift', '~> 1.0'
  pod 'DefaultsKit', '~> 0.2'
  
  # Code Generator / Analyser
  pod 'SwiftGen', '~> 6.0'
  
end

target 'routster' do
  # Pods for routster
  pods
end
